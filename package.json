{
  "name": "svelte-boilerplate",
  "version": "0.0.1",
  "scripts": {
    "start:app": "svelte-kit dev",
    "start:server": "rollup -c rollup.server.config.js -w",
    "start": "NODE_ENV=development concurrently --kill-others \"yarn start:server\" \"yarn start:app\"",
    "build:server": "rollup -c rollup.server.config.js",
    "build:app": "svelte-kit build",
    "build": "yarn build:server && yarn build:app",
    "serve": "node build/index.js",
    "lint": "prettier --check . && eslint --ignore-path .gitignore .",
    "format": "prettier --write .",
    "server": "NODE_ENV=development nodemon --config \"./nodemon.json\""
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-run": "^2.0.2",
    "@rollup/plugin-typescript": "^8.2.1",
    "@sveltejs/kit": "next",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-json": "^2.0.18",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.4.0",
    "@types/koa-send": "^4.1.2",
    "@types/koa-static": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "axios": "^0.21.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.1.0",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "svelte": "^3.29.0",
    "svelte-preprocess": "^4.0.0",
    "typescript": "^4.0.0",
    "tslib": "^2.0.0",
    "vite": "^2.1.0"
  },
  "type": "module",
  "dependencies": {
    "concurrently": "^6.0.2",
    "encoding": "^0.1.13",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-compress": "^5.0.1",
    "koa-connect": "^2.1.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-proxies": "^0.12.1",
    "koa-router": "^10.0.0",
    "koa-send": "^5.0.1",
    "koa-static": "^5.0.0",
    "node-fetch": "^2.6.1"
  }
}
